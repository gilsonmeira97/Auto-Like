{
    "manifest_version": 2,
    "version": "1.0.0",
    "name": "AutoLike",
    "description": "Extensão para curtir os vídeos automáticamente",
    "author": "",
    "browser_action": {
   	 "default_title": "",
   	 "default_popup": "index.html",
   	 "default_icon": "icon.svg"
    },

    "content_scripts": [ {
    "matches": ["https://www.youtube.com/*"],
    "js": ["app.js"]
     }]

}
